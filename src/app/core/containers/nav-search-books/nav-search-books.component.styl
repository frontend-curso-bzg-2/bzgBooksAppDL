	width: 50px;
	height: 50px;
	margin: 15px 15px 15px 15px;

.userImage 
	width: 50px;
	height: 50px;
	border: 1px solid;
	border-radius: 10px 10px 10px 10px;
	font-size: 48px;	
	display: flex;	
	justify-content: center;
	box-shadow: 5px 5px 5px 0px rgba(0,0,0,0.75);

